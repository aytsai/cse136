@{
    var id = ViewBag.Id;
}
<h3>Edit</h3>

<div id="divScheduleTimeEdit">
    Schedule Time : <input type="text" data-bind="value: $data.scheduletime" /><br />
    <br />
    <input type="button" data-bind="value: 'Update', click: $data.update" style="margin-left:80px" />
    <br /><br />
    <div id="divMessage"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        require(['ViewModels/ScheduleTimeViewModel'], function (scheduleTimeViewModel) {
            var scheduleTimeViewModelObj = new scheduleTimeViewModel();
            scheduleTimeViewModelObj.Load("@id");
        });
    });
</script>